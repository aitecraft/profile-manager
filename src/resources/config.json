{
    "_comment": "This file is not meant to be changed. To change settings, edit the prefs.json file instead. Values in this file are default values and should not be changed.",
    
    "api_endpoint": "https://aitecraft.glitch.me/pm/rev_api/dev-index.json",
    
    "defaults": {
        "mc_dir": "<appdata>\\.minecraft",
        "lang": "en",
        "jvm_heap_size": 2048,
        
        "compatibility": {
            "_comment": "These settings help maintain compatibility with third-party / older launchers.",
            "profile_name_is_id": true,
            "create_empty_jar": false
        }
    },
    
    "profile": {
        "id": "Aitecraft",
        "dir": "<mc_dir>\\aitecraft",
        "jvm": {
            "minimum_heap_size": 2048,
            "args": ""
        },
        "misc": {
            "_comment": "This only works with the Official Minecraft Launcher. Path relative to main AU3/EXE.",
            "encoded_icon_file": "resources/aitecraft-icon-base64.txt"
        }
    },
    
    "proprietary": {
        "change_skin_option": true,
        "open_website_option": true,
        "open_schematics_folder_option": true
    },

    "gui": {
        "open_folders_menu": true,
        "optimizer_mod": true,
        "misc": {
            "reinstall_fabric": true,
            "verify_files": true
        },
        "profile_settings": true
    }
}
